{
  "name": "ontouml-schema",
  "version": "1.0.0",
  "description": "This package provides a JSON Schema representation ontologies based on the OntoUML Metamodel. This serialization models in JSON is intended to support the exchange of models over HTTP requests.",
  "main": "src/index.js",
  "scripts": {
    "start": "onchange '**/*.js' '**/*.json' '**/*.yaml' -- prettier --write {{changed}}",
    "generate-and-test": "npm-run-all -s generate test",
    "generate": "npx yaml2json -s src/ontouml-schema.yaml",
    "test": "npx ajv compile -s src/ontouml-schema.json -c ajv-formats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OntoUML/ontouml-schema.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/OntoUML/ontouml-schema/issues"
  },
  "homepage": "https://github.com/OntoUML/ontouml-schema/",
  "dependencies": {},
  "devDependencies": {
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.1",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.0.1",
    "yamljs": "^0.3.0"
  }
}
